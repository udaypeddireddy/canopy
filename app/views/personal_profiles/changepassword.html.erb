        <script type="text/javascript">
        	
        	function validate()
        	
        	{
        		var rgex = /^[0-9a-zA-Z]+$/;
        		var pwd=document.getElementById("password").value;
        		var confirm_pwd =document.getElementById("password_confirmation").value;
        		
        		
        
        		if(document.getElementById("password").value=="")
        		
        		{
        			
        			alert("Please enter Password");
        			document.getElementById("password").focus();
        			return false;
        		}
        		
               else if(rgex.test(pwd) == false)
               {
               alert("Password Must Have Alpha-Numeric Characters Only");
               document.getElementById("password").focus();
               return false;
               }       
        		else if (pwd.length<4)
        		{
        			alert("Password Must Contain Atleast 4 Characters ");
        			document.getElementById("password").focus();
        			return false;
        		}
        	
        		
        		else if(document.getElementById("password_confirmation").value=="")
        		
        		{
        			alert("Please enter Confirmation Password");
        			document.getElementById("password_confirmation").focus();
        			return false;
        		}
        		else if(pwd!=confirm_pwd)
        		{
        			alert("Password And Confirm Password Not Matched")
        			document.getElementById("password_confirmation").focus();
        			return false;
        		}
        		return true;
        		
        	}
        	
        </script>
        
        
